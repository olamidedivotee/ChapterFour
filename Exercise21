package Chapter4;

public class Exercise21 {
    public static void main(String[] args) {

        int x = 9;
        int y = 1;

        if (x < 10)
            if (y > 10)
                System.out.println("*****");
            else
                System.out.println("#####");
        System.out.println("$$$$$");
    }
}
